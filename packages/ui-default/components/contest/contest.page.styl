    .header-center ul .liActive::before {
      content: '';
      /* 必须有内容才能显示伪元素 */
      position: absolute;
      left: 50%;
      bottom: 0;
      /* 放在底部 */
      width: 50%;
      /* 宽度等于父元素宽度 */
      border-bottom: 3px solid #ff7d37;
      /* 边框样式不变 */
      transform: translateX(-50%);
      /* 将元素向左移动自身宽度的一半，实现居中 */
    }

